print(f"Student Name: {school_data.name}")
print(f"Age: {school_data.age}")
print(f"Course Code: {school_data.course_code}")
print(f"Course Name: {school_data.course_name}")
print(f"Total Score: {school_data.total_score}")
print(f"Student Name in UPPERCASE: {school_data.name_upper}")
print(f"Student Name in LOWERCASE: {school_data.name_lower}")
print(f"Student Name length: {school_data.name_length} characters")
print(f"Square Root Of Total Score: {school_data.total}")